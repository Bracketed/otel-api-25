{
	"name": "@bracketed/otel-api",
	"description": "@opentelementery/api but ported to suit modern uses. (2025)",
	"packageManager": "yarn@4.9.2",
	"version": "1.9.0",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"license": "Apache-2.0",
	"author": "@bracketed",
	"main": "lib/cjs/index.cjs",
	"module": "lib/esm/index.mjs",
	"types": "lib/cjs/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./lib/esm/index.d.mts",
				"default": "./lib/esm/index.mjs"
			},
			"require": {
				"types": "./lib/cjs/index.d.ts",
				"default": "./lib/cjs/index.cjs"
			}
		}
	},
	"files": ["lib"],
	"repository": {
		"type": "git",
		"url": "https://github.com/bracketed/otel-api-25"
	},
	"homepage": "https://github.com/bracketed/otel-api-25",
	"sideEffects": false,
	"bugs": {
		"url": "https://github.com/bracketed/otel-api-25/issues"
	},
	"keywords": [
		"bracketed",
		"typescript",
		"opentelemetry",
		"nodejs",
		"browser",
		"tracing",
		"profiling",
		"stats",
		"monitoring"
	],
	"scripts": {
		"build": "tsup",
		"update": "yarn upgrade-interactive"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@bracketed/tsup-configuration": "^1.0.5",
		"@bracketed/typescript-config": "^4.7.1",
		"@swc/core": "^1.12.5",
		"@types/node": "^24.0.3",
		"esbuild": "^0.25.5",
		"esbuild-fix-imports-plugin": "^1.0.21",
		"esbuild-plugin-copy": "^2.1.1",
		"esbuild-plugin-file-path-extensions": "^2.1.4",
		"esbuild-plugin-version-injector": "^1.2.1",
		"tslib": "^2.8.1",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"read-package-up": "^11.0.0"
	}
}
